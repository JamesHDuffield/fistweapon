.panel.panel-primary
  .panel-heading
    .row
      .col-sm-12
        h4.panel-title Members

  .panel-body
    table.table.table-hover
      thead
        tr
          th Spec
          th Name
          th Level
          th Rank
          th YR
      tbody
        - @members.each do |m|
          tr
            td
              img.wow-icon(src=m.spec_url)
            td
              a.c(class="c#{m.character_class}" href="#{m.ask_mr_robot}") = m.name
            td = m.level
            td = m.rank_name
            td = render_label(m)
